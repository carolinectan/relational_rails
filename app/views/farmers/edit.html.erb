<%= form_with url: "/farmers/#{@farmer.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %><br>
  <%= form.text_field :name %><br><br>
  <%= form.label :age %><br>
  <%= form.text_field :age %><br><br>
  <%= form.label :owns_land %><br>
  <%= form.check_box :owns_land %><br><br>
  <%= form.submit "Edit Farmer #{@farmer.name}" %>
<% end %>
