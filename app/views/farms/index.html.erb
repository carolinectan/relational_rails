  <a href='/farmers'>Farmer Index</a>
  <a href='/gardens'>Garden Index</a>
  <a href='/plants'>Plants Index</a>
</nav>

<h1>All Farms</h1>

<% @farms.each do |farm| %>
  <h3><a href="/farms/<%= farm.id %>"><%= farm.name %></a></h3>
  <p>(Created at: <%= farm.created_at %>)</p>
  <%= button_to "Update #{farm.name}", "/farms/#{farm.id}", method: :get %>
<% end %>
<br>
<button><%= link_to "New Farm", "/farms/new" %></button>
